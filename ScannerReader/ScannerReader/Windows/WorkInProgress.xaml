<Window x:Class="ScannerReader.Windows.WorkInProgress"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Icon="{StaticResource WaitIcon}"
        mc:Ignorable="d" ResizeMode="NoResize" WindowStyle="None"
        Title="WorkInProgress" Height="75.664" Width="336.504" Loaded="WorkInProgress_OnLoaded">
    
    <Grid>
        <DockPanel>
            <ProgressBar Name="ProgressBar" DockPanel.Dock="Top" Value="0" Minimum="0" Maximum="100" Height="60px"></ProgressBar>
            <TextBlock Name="ProgressMessage" DockPanel.Dock="Bottom" Height="16px" FontSize="10px"></TextBlock>
        </DockPanel>
    </Grid>
</Window>
